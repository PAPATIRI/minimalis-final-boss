{
  "name": "minimalis final boss",
  "type": "light",
  "colors": {
    "editor.background": "#faf4ed",
    "editor.foreground": "#333333",
    "editorLineNumber.foreground": "#b8b2a6",
    "editorCursor.foreground": "#d7827e",
    "editor.selectionBackground": "#f2e9e1",

    "sideBar.background": "#fffaf3",
    "activityBar.background": "#fffaf3",
    "statusBar.background": "#fffaf3",
    "titleBar.activeBackground": "#fffaf3",

    "editorGroupHeader.tabsBackground": "#fffaf3",
    "tab.activeBackground": "#faf4ed",
    "tab.inactiveBackground": "#f2e9e1",

    "panel.background": "#fffaf3",
    "editorGutter.background": "#faf4ed",
    "editorWidget.background": "#faf4ed",
    "input.background": "#ffffff",

    "editorBracketHighlight.foreground1": "#333333",
    "editorBracketHighlight.foreground2": "#333333",
    "editorBracketHighlight.foreground3": "#333333",
    "editorBracketHighlight.foreground4": "#333333",
    "editorBracketHighlight.foreground5": "#333333",
    "editorBracketHighlight.foreground6": "#333333",
    "editorBracketMatch.border": "#d7827e"
  },

  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#6e6a86" }
    },
    {
      "name": "Strings",
      "scope": ["string", "constant.other.symbol"],
      "settings": { "foreground": "#b4637a" }
    },
    {
      "name": "Identifiers (class, method, variable, etc)",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.scope-resolution",
        "entity.name.function",
        "entity.name.method",
        "entity.name.property",
        "entity.name.label",
        "entity.name.tag",
        "support.type",
        "support.class",
        "support.function",
        "meta.function-call",
        "variable.other.property",
        "variable.other.member"
      ],
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "name": "TODO",
      "scope": ["keyword.other.todo", "comment.block.todo"],
      "settings": { "foreground": "#EB6F92" }
    }
  ]
}
